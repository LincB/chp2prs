defproc assign3 () {
  int<1> x;
  int<1> y;
  int<1> z;
  int<1> v;
  int<1> a;
  int<1> b;

  chp { 
    y:=1;
    x:=(y = x);   // 0
    v:=(x < y);   // 1
    z:=(x > y);   // 0
    a:=(v >= y);  // 1
    b:=(x <= y);  // 1
    y:=(v != b);  // 0
    x:=(z != y);  // 1
    y:=1
  }
}

assign3 a;
