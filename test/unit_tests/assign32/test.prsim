initialize
watch Reset
watch _Reset
watch t.go.r
watch t.var_x.v.t
watch t.var_x.v.f
watch t.go.a
set_principal t.go.r
set_principal t.var_x.v.t
set_principal t.var_x.v.f
set_principal t.go.a

mode reset
set Reset 1
set _Reset 0
set t.go.r 0
set t.go.a 0
cycle
assert t.go.r 0
assert t.go.a 0
set Reset 0
set _Reset 1
cycle
mode run
cycle

assert t.var_x.v.t 0
assert t.var_x.v.f 1
set t.go.r 1
cycle

assert t.go.r 1
assert t.var_x.v.t 1
assert t.var_x.v.f 0
assert t.go.a 1
